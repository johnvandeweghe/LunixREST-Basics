{
  "name": "johnvandeweghe/lunixrest-basocs",
  "description": "Basic implementations of LunixREST",
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "authors": [
    {
      "name": "John VanDeWeghe",
      "email": "vandeweghe@lunixlabs.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "johnvandeweghe/lunixrest": "dev-master",
    "guzzlehttp/psr7": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "LunixRESTBasics\\": "src/"
    }
  },
  "suggest": {
    "doctrine/doctrine": "Required for implementations that use DoctrineEndpointFactory and DoctrineEndpoint"
  }
}
